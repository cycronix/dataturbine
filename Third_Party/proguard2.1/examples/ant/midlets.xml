<!-- This Ant build file illustrates how to process J2ME midlets.
     Usage: ant -f examples/ant/midlets.pro -->

<project name="Midlets" default="obfuscate" basedir="../..">

<target name="obfuscate">
   <taskdef name="proguard" classname="proguard.ant.ProGuardTask"
            classpath="/usr/local/java/proguard/lib/proguard.jar" />
   <proguard printseeds="on"
             outjar="out.jar">
      <libraryjar name="${java.home}/lib/rt.jar" />
      <libraryjar name="/usr/local/java/wtk104/lib/midpapi.zip" />
      <!-- Specify your library jars here. -->
      <!-- Specify your input jars here. -->
      <injar name="in.jar" />

      <!-- Preserve all public midlets. -->
      <keep access="public" extends="javax.microedition.midlet.MIDlet" />

      <!-- Your application may contain more items that need to be preserved;
        typically classes that are dynamically created using Class.forName -->
   </proguard>
</target>

</project>
